{
  "name": "@picturae/openseadragonzoomlevels",
  "version": "1.0.0",
  "description": "An OpenSeadragon plugin to allow restricting the image zoom to specific levels.",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild --bundle --platform=browser --sourcemap --minify ./src/zoom-levels.js --outfile='./dist/openseadragonzoomlevels.js'",
    "dev": "yarn build --watch --servedir=."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/picturae/OpenSeadragonZoomLevels.git"
  },
  "keywords": [
    "openseadragon",
    "zoom",
    "levels"
  ],
  "author": "Petar Petrov",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/picturae/OpenSeadragonZoomLevels/issues"
  },
  "homepage": "https://github.com/picturae/OpenSeadragonZoomLevels#readme",
  "dependencies": {
    "openseadragon": "2.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.2"
  }
}
